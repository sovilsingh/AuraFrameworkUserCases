<aura:component   >
    <aura:handler  name="init" value="{!this}" action="{!c.doinit}"/>
    <aura:attribute type="map" name="map"/>
    <aura:attribute type="string" name="key"/>
    <aura:attribute type="string" name="value"/>
    {!v.key}-------{!v.value}  // Displaying Key,Value.
</aura:component>